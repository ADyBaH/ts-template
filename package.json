{
  "name": "ts-template-project",
  "version": "1.0.0",
  "description": "ts-template-project",
  "main": "index.ts",
  "scripts": {
    "deploy-root": "npm run build && npx gh-pages -d dist",
    "deploy-folder": "npm run build && npx gh-pages -d dist -e folder",
    "prepare": "husky install",
    "sl": "stylelint **/*.scss --fix",
    "l": "eslint . --color",
    "b": "webpack --config webpack.prod.js",
    "s": "webpack serve --config webpack.dev.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(./.husky)**/*.{js,jsx,ts,tsx}": [
      "npm run l"
    ]
  },
  "author": "ADyBaH",
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "normalize.css": "^8.0.1",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^15.6.1",
    "stylelint-config-clean-order": "^5.0.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-webpack-plugin": "^4.1.1",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.7.3"
  }
}
